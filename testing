CREATE TABLE Artist(
ArtistID INT,
ArtistFullName VARCHAR(60),
ArtistDOB DATE,
PosterInStock BOOLEAN
);

insert into artist(ArtistID, ArtistFullName, ArtistDOB, PosterInStock)
values('10001','Jennifer Warnes','1947-03-03','False');

insert into artist(ArtistID, ArtistFullName, ArtistDOB, PosterInStock)
values('10002','Joni Mitchell','1943-11-07','False');

insert into artist(ArtistID, ArtistFullName, ArtistDOB, PosterInStock)
values('10003','Bing Crosby','1904-05-02','True');

