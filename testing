values('10008','Placido Domingo','1941-01-21','False');

select * from artist;

CREATE TABLE CompactDiscs(
    CompactDiscID INT,
    CDTitle VARCHAR(60),
    LabelID INT
);

CREATE TABLE CDLabels(
 LabelID INT,
 CompanyName VARCHAR(60)
);

CREATE TABLE MusicTypes(
 TypeID INT,
 TypeName VARCHAR(20)
);

